{
  "name": "webpack-babel-config",
  "version": "1.0.0",
  "description": "webpack and babel configuration",
  "main": "index.js",
  "scripts": {
    "build": "babel-webpack",
    "start:dev": "webpack-dev-server",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node buildScript/srcServer",
    "lint": "esw webpack.config.* src buildScript --color",
    "lint:watch": "npm run lint -- --watch",
    "share": "npm-run-all -p open:src localtunnel",
    "test": "mocha --reporter progress buildScript/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "webpack",
    "babel"
  ],
  "author": "Makana Brian",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.0.0",
    "global-jsdom": "6.1.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "16.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.4.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
